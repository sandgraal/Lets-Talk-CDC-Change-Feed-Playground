{
  "id": "crud-basic",
  "name": "CRUD Basic",
  "description": "Insert, update, and delete a single customer to highlight delete visibility.",
  "tags": [
    "crud",
    "polling",
    "basics"
  ],
  "ops": [
    {
      "t": 100,
      "op": "insert",
      "table": "customers",
      "pk": {
        "id": "1"
      },
      "after": {
        "name": "A",
        "email": "a@example.com"
      }
    },
    {
      "t": 400,
      "op": "update",
      "table": "customers",
      "pk": {
        "id": "1"
      },
      "after": {
        "name": "A1",
        "email": "a@example.com"
      }
    },
    {
      "t": 700,
      "op": "delete",
      "table": "customers",
      "pk": {
        "id": "1"
      }
    }
  ]
}